#include <stdio.h>

int main() {
    int start, end, i, sum = 0;

    printf("Enter the start of the range: ");
    scanf("%d", &start);

    printf("Enter the end of the range: ");
    scanf("%d", &end);

    for(i = start; i <= end; i++) {
        if(i % 2 == 0) {   
         sum += i;
        }
    }

    printf("Sum of even numbers from %d to %d is: %d\n", start, end, sum);

    return 0;
}
